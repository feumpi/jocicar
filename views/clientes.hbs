<html data-theme='light'>
  <head>
    <meta charset='utf-8' />
    <title>Clientes - Jocicar</title>
    <script src='https://cdn.tailwindcss.com'></script>
    <link
      href='https://cdn.jsdelivr.net/npm/daisyui@4.12.13/dist/full.min.css'
      rel='stylesheet'
      type='text/css'
    />
  </head>
  <body>

    <table class='table table-zebra'>

      <thead>
        <tr>
          <th>Nome</th>
          <th>CPF</th>
          <th>CNH</th>
          <th>Telefone</th>
          <th>E-mail</th>
          <th>Endere√ßo</th>
        </tr>
      </thead>

      <tbody>
        {{#each clientes}}
          <tr>
            <td>{{this.nome}}</td>
            <td>{{this.cpf}}</td>
            <td>{{this.cnh}}</td>
            <td>{{this.telefone}}</td>
            <td>{{this.email}}</td>
            <td>{{this.endereco}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>

  </body>
</html>